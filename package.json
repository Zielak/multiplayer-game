{
  "name": "multiplayer-game",
  "version": "0.0.3",
  "description": "just testing multiplayer game with server & client",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "server": "node index.js",
    "server:dev": "nodemon",
    "precommit": "npm test && npm run lint",
    "lint": "./node_modules/.bin/eslint ./ -f visualstudio",
    "test": "ava",
    "test:cover": "nyc ava",
    "test:watch": "ava --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colyseus": "^0.9.9",
    "colyseus.js": "^0.9.5",
    "express": "^4.16.3",
    "pixi.js": "^4.8.0",
    "prop-types": "^15.6.1",
    "serve-index": "^1.9.1",
    "transformation-matrix": "^1.10.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/pixi.js": "^4.7.3",
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eventemitter3": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "nyc": "^11.8.0",
    "sass-loader": "^7.0.1",
    "schema-utils": "^0.4.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "app/*",
      "client/*",
      "shared/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "report-dir": "./dist/coverage"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js",
    "watch": [
      "app/"
    ]
  }
}
