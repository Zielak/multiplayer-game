{
  "name": "multiplayer-game",
  "version": "0.0.2",
  "description": "just testing multiplayer game with server & client",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "server": "node index.js",
    "server:dev": "nodemon",
    "precommit": "npm test && npm run lint",
    "lint": "./node_modules/.bin/eslint ./ -f visualstudio",
    "test": "nyc ava",
    "test:watch": "ava --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colyseus": "^0.5.9",
    "colyseus.js": "^0.7.4",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "redux": "^3.7.2",
    "serve-index": "^1.9.1",
    "transformation-matrix": "^1.5.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "sass-loader": "^6.0.6",
    "schema-utils": "^0.3.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "app/*",
      "client/*",
      "shared/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "report-dir": "./dist/coverage"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js",
    "watch": [
      "app/"
    ]
  }
}
